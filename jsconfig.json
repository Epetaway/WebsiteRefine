{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/lib/*": ["src/lib/*"],
      "@/data/*": ["src/data/*"],
      "@/images/*": ["src/images/*"],
      "@/styles/*": ["src/styles/*"]
    }
  },
  "exclude": ["node_modules", ".next", "out"]
}